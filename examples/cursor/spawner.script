local function random_pos()
	return vmath.vector3(math.random(1136), math.random(640), math.random(-100, 100) / 100)
end

function init(self)
	math.randomseed(socket.gettime())
	for i=1,200 do
		factory.create("#bluefactory", random_pos())
	end
	for i=1,20 do
		factory.create("#greenfactory", random_pos())
	end
end
